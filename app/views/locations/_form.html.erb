<div class="container">
<%= semantic_form_for @location do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :logo %>
    <%= f.input :region, as: :select, collection: ['North Tahoe','South Tahoe','Central CA','Southern CA','Northern California','Utah','Colorado','Wyoming','Idaho','Oregon','Washington','New Mexico'] %>
    <%= f.input :state, as: :select, collection: ['California','Utah','Colorado','Wyoming','Idaho','Oregon','Washington','New Mexico'] %>
    <%= f.input :partner_status, as: :select, collection: ['Active','Inactive'] %>
    <%= f.input :calendar_status, as: :select, collection: ['Regular','Peak','Peak Plus'] %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
</div>
