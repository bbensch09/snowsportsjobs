<div class="container">
<p id="notice"><%= notice %></p>
<div class="row">
      <%= image_tag @contestant.avatar.url(:large), alt:"avatar-img", class:" center img-responsive instructor-profile-photo" %>
</div>
  
<p>
  <strong>Username:</strong>
  <%= @contestant.username %>
</p>

<p>
  <strong>Hometown:</strong>
  <%= @contestant.hometown %>
</p>

<div class="selfies-list">
<% @contestant.selfies.each do |selfie| %>
<div class="col-md-4">
<h1>Here is a selfie:</h1>
<img src="" alt="user generated selfie">
<% end %>
</div>

<%= link_to 'Edit', edit_contestant_path(@contestant) %> |
<%= link_to 'Back', contestants_path %>
</div>